echo $OSTYPE
if [[ $OSTYPE == darwin* ]]; then
  # note: only installing omnisharp on mac for now
  python ~/.vim/plugged/YouCompleteMe/install.py --clang-completer --tern-completer --omnisharp-completer
else
  python3 ~/.vim/plugged/YouCompleteMe/install.py --clang-completer --tern-completer  --system-libclang
fi
