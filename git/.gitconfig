[core]
  excludesfile = ~/.gitignore_global
  editor = vim
[user]
  name = Gerrard Lindsay
  email = gerrard.lindsay@kaplan.com
[push]
  default = simple
[difftool "kdiff3"]
    path = /usr/local/bin/kdiff3
    trustExitCode = false
[difftool]
    prompt = false
[diff]
    tool = kdiff3
[mergetool "kdiff3"]
    path = /usr/local/bin/kdiff3
    trustExitCode = false
[mergetool]
    keepBackup = false
[merge]
    tool = kdiff3
